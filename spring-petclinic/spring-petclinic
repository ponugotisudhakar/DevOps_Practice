node('ip-172-31-6-55.ap-south-1.compute.internal'){

    stage('message'){
        echo 'Hello Spring-Petclinic'
    }
   
    stage('git clone'){
        git 'https://github.com/spring-projects/spring-petclinic.git'

    }
    stage('mvn pakcage'){
        sh 'mvn pakcage'

    }
    stage('archive'){
        archiveArtifacts 'target/surefire-reports/*.xml'

    }
}